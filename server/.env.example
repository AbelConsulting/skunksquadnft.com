# Printful API Configuration
# Get these from your Printful App settings at: https://www.printful.com/dashboard/app

# Your Printful API Token (for direct API access)
PRINTFUL_API_TOKEN=your_api_token_here

# Your Printful Store ID (optional, for V2 multi-store support)
PRINTFUL_STORE_ID=

# OAuth credentials (if using Printful App OAuth)
PRINTFUL_CLIENT_ID=your_client_id_here
PRINTFUL_CLIENT_SECRET=your_client_secret_here

# Server Configuration
PORT=3001
