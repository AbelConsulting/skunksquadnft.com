    },
    {
      "src": "icon-192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any maskable"
    },
    {
      "src": "icon-512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "screenshots": [
    {
      "src": "screenshot-wide.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide"
    },
    {
      "src": "screenshot-mobile.png",
      "sizes": "375x812",
      "type": "image/png"
    }
  ],
  "shortcuts": [
    {
      "name": "Connect Wallet",
      "short_name": "Wallet",
      "description": "Connect your Web3 wallet",
      "url": "/#wallet",
      "icons": [
        {
          "src": "favicon.ico",
          "sizes": "32x32"
        }
      ]
    },
    {
      "name": "Analytics",
      "short_name": "Stats",
      "description": "View collection analytics",
      "url": "/#analytics",
      "icons": [
        {
          "src": "favicon.ico",
          "sizes": "32x32"
        }
      ]
    }
