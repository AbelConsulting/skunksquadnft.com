  "edge_side_panel": {
    "preferred_width": 400
  },
  "scope": "/",
  "id": "skunksquad-nft",
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+nft",
      "url": "/?nft=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/",
      "accept": {
        "image/*": [".png", ".jpg", ".jpeg", ".gif", ".webp"]
      }
    }
  ]
