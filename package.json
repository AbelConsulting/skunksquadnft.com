{
  "name": "skunk-squad-nft",
  "version": "1.0.0",
  "description": "Skunk Squad NFT Collection Smart Contract",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "deploy:localhost": "hardhat run scripts/deploy.js --network localhost",
    "deploy:sepolia": "hardhat run scripts/deploy.js --network sepolia",
    "deploy:mainnet": "hardhat run scripts/deploy.js --network mainnet",
    "deploy-enhanced": "hardhat run scripts/deploy-enhanced.js --network sepolia",
    "deploy-ultra": "hardhat run scripts/deploy-ultra-smart.js --network sepolia",
    "deploy-payment-gateway": "hardhat run scripts/deploy-payment-gateway.js --network sepolia",
    "deploy-payment-mainnet": "hardhat run scripts/deploy-payment-gateway.js --network mainnet",
    "test-contract": "hardhat run scripts/test-contract.js --network sepolia",
    "test-ultra": "hardhat run scripts/test-ultra-smart.js --network sepolia",
    "verify:sepolia": "hardhat verify --network sepolia",
    "verify:mainnet": "hardhat verify --network mainnet",
    "node": "hardhat node",
    "clean": "hardhat clean",
    "setup-opensea": "hardhat run scripts/setup-opensea.js --network sepolia",
    "metadata-server": "node scripts/metadata-server.js",
    "update-uris": "hardhat run scripts/update-uris.js --network sepolia",
    "analytics": "node scripts/analytics-dashboard.js",
    "dashboard": "node scripts/analytics-dashboard.js",
    "payment-server": "node scripts/payment-server.js",
    "payment-dev": "NODE_ENV=development node scripts/payment-server.js",
    "setup-payments": "npm install stripe && echo 'Payment dependencies installed! Configure .env and run: npm run deploy-payment-gateway'",
    "start-payment-system": "npm run deploy-payment-gateway && npm run payment-server"
  },
  "keywords": [
    "nft",
    "ethereum",
    "solidity",
    "erc721a",
    "hardhat"
  ],
  "author": "Skunk Squad Team",
  "license": "MIT",
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@typechain/ethers-v5": "^10.2.1",
    "@typechain/hardhat": "^6.1.6",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.3",
    "chai": "^4.3.7",
    "ethers": "^5.7.2",
    "hardhat": "^2.14.0",
    "hardhat-gas-reporter": "^1.0.9",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.9.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "erc721a": "^4.2.3",
    "express": "^4.21.2",
    "merkletreejs": "^0.3.11",
    "stripe": "^14.21.0"
  }
}
